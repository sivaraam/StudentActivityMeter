# http://editorconfig.org

# indicate this is the root of the project
root = true

[*]
charset = utf-8
indent_style = space
indent_size = 2
end_of_line = lf
insert_final_newline = true
trim_trailing_whitespace = true

[*.md]
trim_trailing_whitespace = true

[*.java]
max_line_length = 100
continuation_indent_size = 4
